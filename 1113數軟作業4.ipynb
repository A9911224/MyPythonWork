     "text": [
      "想知道哪年母親節的日期呢：2030\n",
      "2030-05-12\n"
     ]
    }
   ],
   "source": [
    "a = input(\"想知道哪年母親節的日期呢：\")\n",
    "\n",
    "\n",
    "def mom(x,y):\n",
    "    return datetime.date(x,5,y)\n",
    "for i in range(int(a),int(a)+1):\n",
    "    for j in range(8,15):\n",
    "        if mom(i,j).strftime(\"%a\") == \"Sun\":\n",
    "            print(mom(i,j))"
   ]
